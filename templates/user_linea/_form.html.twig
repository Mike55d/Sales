{% form_theme form 'formTemplate.html.twig' %}
{% block css %}
<style>
	label {
		margin-right: 25px;
		margin-top: 10px;
	}
	input {
		margin-bottom: 15px;
	}
</style>
{% endblock %}

<div class="container">
	<div class="card">
		<div class="card-body">
			{{ form_start(form) }}
			{{ form_widget(form) }}
			<button class="btn btn-success">{{ button_label|default('Save') }}</button>
			{{ form_end(form) }}
		</div>
	</div>
</div>